<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.eleven">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HomeWork">
        <activity android:name=".view.edit.EditContactActivity"/>

        <activity android:theme="@style/AppThemeWithButtonActionBar" android:name=".view.add.AddContactActivity"/>
        <activity android:name=".view.list.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <provider
            android:authorities="com.example.eleven"
            android:name=".model.ContactsProvider"
            android:exported="true"/>

    </application>

</manifest>